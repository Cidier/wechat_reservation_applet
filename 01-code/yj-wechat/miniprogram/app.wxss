@import 'mmwxss/mm.wxss';
/**app.wxss**/

.weui-cell{
  padding: 10px 16px;
}
.weui-cells{
  position:static;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  /* padding-top: 5rem; */
} 
.container-page {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
button{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.button-sp-area{
  margin: 0 auto;
  width: 60%;
}
.mini-btn{
  margin-right: 10rpx;
}




page {
  width: 100%;
  height: 100%;
  position: relative;
  color: rgba(0, 0, 0, .85);
  background-color: #f8f8f8;
  font-size: 16px;
  font-family: PingFang SC, Helvetica Neue, Hiragino Sans GB, Helvetica, Microsoft YaHei, Arial;
  -ms-text-size-adjust: 100%;
  -webkit-text-size-adjust: 100%;
  -webkit-tap-highlight-color: transparent;
}
