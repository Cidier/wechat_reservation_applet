.map-before{
  text-indent: 1rem;
  color: #aaa;
  font-size: 0.8rem;
}

.map-before::before{
  /*必须添加content属性，否则后期不可见*/
  content:"";
  /*默认是行级元素，如果想设置宽高，必须转换为块级元素  : position定位*/
  position: absolute;
  width: 16px;
  height: 16px;
  background-size: 16px;
  background-image: url("data:image/png;base64,");
  left: 8px;
}
